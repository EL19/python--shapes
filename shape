from turtle import *
import math

# Name your Turtle.
t = Turtle()

# Set Up your screen and starting position.


setup(1000,1000)
x_pos = 0
y_pos = 0
t.setposition(x_pos, y_pos)

### Write your code below:
t.showturtle()
color('green')
t.pendown()
t.forward(120)
left(90)
forward(120)
left(90)
forward(120)
left(90)
forward(120)
left(90)

color('alice blue')

end_fill()
penup()
reset()
# Close window on click.
exitonclick()
